@model ResponseModel

@if (Model != null)
{
    <div class="alert @Model.StyleCssClass alert-styled-left alert-arrow-left alert-dismissible">
        <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
        <span class="font-weight-semibold">@Model.Title</span> @Model.Message
    </div>
}